        <onclick/>
        <order>100</order>
        <script><![CDATA[// Check if the "Requested For" field is not empty
if (current.requested_for) {
    // Queue the event 'send_notification_event'
    gs.eventQueue('send_workout_plan', current, current.requested_for.toString(), gs.getUserID());
    
    // Provide feedback to the user
    gs.addInfoMessage('Notification has been sent to ' + current.requested_for.getDisplayValue());
} else {
    gs.addErrorMessage('Requested For field is empty.');
}
]]></script>
