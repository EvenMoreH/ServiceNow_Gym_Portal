        <query_from/>
        <query_with><![CDATA[(function refineQuery(current, parent) {
    // Check if the current record (member) has the role 'instructor'
    if (current.role == 'instructor') {
        // If an instructor, add a query to match member name with owner in the Skills table
        current.addQuery('title', current.instructor);
    } else {
        // If not an instructor, add a query that will return no records
        current.addQuery('number', '1=2'); // This is a dummy condition that will always be false
    }
})(current, parent);]]></query_with>
